#include <iostream>
#include <cctype>
#include <string>
#include <algorithm>
#include <map>

using namespace std;
long long fiboarr[40] = {0, 1};
long long fibonacci(int n) {
	if (n == 0 || n == 1) {
		return fiboarr[n];
	}
	else {
		fiboarr[n] = fibonacci(n - 1) + fibonacci(n - 2);
	}
	return fiboarr[n];
}

int main()
{
	int n;
	cin >> n;

	cout << fibonacci(n) << " " << n - 2 << endl;
	

}