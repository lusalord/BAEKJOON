#include <iostream>
using namespace std;

int main() 
{
    string a;
    cin >> a;

    string n(a.length(), ' ');
    string m(a.length(), ' ');

    for (int i = 0; i < a.length(); i++)
    {
        n[i] = a[i];
    }

    for (int i = 0; i < a.length(); i++)
    {
        m[i] = a[a.length() - 1 - i];
    }

    if (n == m) {
        cout << "1";
    }
    else {
        cout << "0";
    }
}
