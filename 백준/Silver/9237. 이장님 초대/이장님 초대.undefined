#include <iostream>
#include <algorithm>
using namespace std;

int main()
{
	int n;
	cin >> n;

	int* arr = new int[n];

	int cnt = 0;

	for (int i = 0; i < n; i++) {
		cin >> arr[i];
	}

	sort(arr, arr + n, greater<int>());

	for (int i = 0; i < n; i++) {
		cnt = max(cnt, arr[i] + i + 1);
	}

	cout << cnt + 1;
}