#include <iostream>
#include <algorithm>
#include <vector>
using namespace std;

int main()
{
	int n, m;
	cin >> n >> m;

	int* coins = new int[n];

	for (int i = 0; i < n; i++)
	{
		cin >> coins[i];
	}

	sort(coins, coins + n, greater<int>());

	int count = 0;

	for (int i = 0; i < n; i++)
	{
		if (m == 0)
			break;
		count += m / coins[i];
		m %= coins[i];
	}

	cout << count;

}